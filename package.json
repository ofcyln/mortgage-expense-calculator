{
  "name": "mortgage-expense-calculator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod --aot --build-optimizer",
    "deploy:firebase": "firebase deploy",
    "format": "prettier --single-quote --trailing-comma all --print-width 120 --arrow-parens always --tab-width 4 --write",
    "tslint": "tslint \"src/**/*.ts\"",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run format",
      "npm run tslint",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run stylelint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ofcyln/mortgage-expense-calculator.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.7",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.7",
    "@angular/compiler": "^9.1.7",
    "@angular/core": "^9.1.7",
    "@angular/flex-layout": "^9.0.0-beta.30",
    "@angular/forms": "^9.1.7",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "^9.1.7",
    "@angular/platform-browser-dynamic": "^9.1.7",
    "@angular/router": "^9.1.7",
    "@angular/service-worker": "^9.1.7",
    "ngx-mask": "^9.0.2",
    "rxjs": "~6.5.4",
    "rxjs-tslint": "^0.1.8",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.6",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^9.1.7",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.39",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "husky": "^4.2.5",
    "stylelint": "^13.3.3",
    "lint-staged": "^10.2.2",
    "stylelint-config-standard": "^20.0.0",
    "prettier": "^2.0.5",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
